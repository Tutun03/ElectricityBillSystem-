CREATE TABLE bills (
bill_id INT PRIMARY KEY GENERATED ALWAYS AS IDENTITY,
bill_no VARCHAR(20),
user_id VARCHAR(20),
amount DOUBLE,
status VARCHAR(10) DEFAULT 'unpaid'
);

